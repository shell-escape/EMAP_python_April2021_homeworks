[flake8]

exclude =
    *_venv
    venv_*
    venv
    ./homework_12/homework_12/* # Django stuff
    ./homework_12/task/*

max-cognitive-complexity = 10
per-file-ignores =
    test*.py: S101
    # Skip unused imports in __init__ files
    __init__.py: F401

max_line_length = 88
max_doc_length = 72

doctests = True
